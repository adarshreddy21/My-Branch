<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Confirmed_App_Set_should_be_Numeric</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(TEXT(Confirmed_Appts_Set__c),&quot;^[0-9]*$&quot;))</errorConditionFormula>
    <errorDisplayField>Confirmed_Appts_Set__c</errorDisplayField>
    <errorMessage>Please Enter Numeric Values Only</errorMessage>
</ValidationRule> 
 