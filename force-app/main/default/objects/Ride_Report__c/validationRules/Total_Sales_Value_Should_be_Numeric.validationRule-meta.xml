<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Sales_Value_Should_be_Numeric</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(TEXT(Total_Sales_Meetings_Completed__c),&quot;^[0-9]*$&quot;))</errorConditionFormula>
    <errorDisplayField>Total_Sales_Meetings_Completed__c</errorDisplayField>
    <errorMessage>Please Enter Numeric Values Only</errorMessage>
</ValidationRule>
