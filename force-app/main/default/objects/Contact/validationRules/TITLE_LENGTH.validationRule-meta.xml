<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TITLE_LENGTH</fullName>
    <active>true</active>
    <description>Lenth of title cannot be more than 60 characters</description>
    <errorConditionFormula>LEN( Title )&gt;60</errorConditionFormula>
    <errorDisplayField>Title</errorDisplayField>
    <errorMessage>The title is too long</errorMessage>
</ValidationRule>
