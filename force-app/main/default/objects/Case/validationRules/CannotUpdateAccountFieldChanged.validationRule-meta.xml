<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CannotUpdateAccountFieldChanged</fullName>
    <active>true</active>
    <description>This validation rule restricts the edits of Account Field Changed.</description>
    <errorConditionFormula>AND(NOT(ISNEW()),
ISCHANGED(Account_Field_Changed__c ) )</errorConditionFormula>
    <errorDisplayField>Account_Field_Changed__c</errorDisplayField>
    <errorMessage>You are not allowed to change this field.</errorMessage>
</ValidationRule>
