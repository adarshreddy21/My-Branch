            <targetReference>Links_No_WIN_ID</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Account Doesn&apos;t Have WIN ID</defaultConnectorLabel>
        <rules>
            <name>Account_Has_WIN_ID</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Account.WIN_ID__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Links_WIN_ID</targetReference>
            </connector>
            <label>Account Has WIN ID</label>
        </rules>
